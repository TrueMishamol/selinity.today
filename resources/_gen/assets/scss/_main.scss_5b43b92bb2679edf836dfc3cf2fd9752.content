@keyframes scroll {
  0% {
    transform: translateX(0%);
    /* Начальная позиция справа */ }
  100% {
    transform: translateX(-100%);
    /* Конечная позиция слева */ } }

.marquee {
  display: flex;
  width: 100%; }
  .marquee_group, .marquee_duplicate {
    animation: scroll 50s infinite linear;
    height: fit-content;
    min-width: fit-content;
    display: flex; }
    .marquee_group > *, .marquee_duplicate > * {
      margin-left: var(--px32px); }
      .marquee_group > * img, .marquee_duplicate > * img {
        width: var(--px256px);
        height: var(--px256px);
        user-select: none;
        transition: transform 0.3s ease-in-out; }
        .marquee_group > * img:hover, .marquee_duplicate > * img:hover {
